{
  "name": "modjules",
  "version": "0.1.1",
  "engines": {
    "node": ">=18"
  },
  "workspaces": [
    ".",
    "examples/*"
  ],
  "description": "The agent-ready SDK for Jules.",
  "repository": "davideast/modjules",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./browser": {
      "types": "./dist/browser.d.ts",
      "import": "./dist/browser.mjs"
    },
    "./gas": {
      "types": "./dist/gas/index.d.ts",
      "import": "./dist/gas/index.mjs"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server.mjs"
    },
    "./server/auth/firebase": {
      "types": "./dist/server/auth/firebase.d.ts",
      "import": "./dist/server/auth/firebase.mjs"
    },
    "./server/auth/memory": {
      "types": "./dist/server/auth/memory.d.ts",
      "import": "./dist/server/auth/memory.mjs"
    },
    "./mcp": {
      "types": "./dist/mcp/index.d.ts",
      "import": "./dist/mcp.mjs"
    },
    "./types": {
      "types": "./dist/types.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "scripts": {
    "build": "vite build",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "pretest": "npm run build",
    "test": "vitest --exclude tests/storage/browser.test.ts",
    "test:browser": "vitest run -c vitest.browser.config.ts",
    "test:integration": "vitest run -c vitest.integration.config.ts",
    "test:smoke": "tsx scripts/smoke-test.ts",
    "type-check": "tsc --project tsconfig.test.json",
    "verify": "tsx scripts/verify.ts",
    "prepare": "husky",
    "changeset": "changeset",
    "release": "changeset publish"
  },
  "keywords": [
    "jules",
    "ai",
    "agent",
    "sdk",
    "developer-tools"
  ],
  "author": "",
  "license": "ISC",
  "lint-staged": {
    "**/*": "prettier --write ."
  },
  "prettier": {
    "semi": true,
    "trailingComma": "all",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.7",
    "@google/jules": "^0.1.37",
    "@inquirer/prompts": "^7.9.0",
    "@octokit/rest": "^22.0.1",
    "@opentelemetry/api": "^1.9.0",
    "@types/google-apps-script": "^2.0.7",
    "@types/node": "^24.9.1",
    "fake-indexeddb": "^6.2.5",
    "firebase-admin": "^13.6.0",
    "husky": "^9.1.7",
    "jsdom": "^27.1.0",
    "lint-staged": "^16.2.6",
    "msw": "^2.11.6",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.1",
    "idb": "^8.0.3"
  }
}
