{
  "name": "julets",
  "version": "0.0.1",
  "description": "The agent-ready SDK for Jules.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest --exclude tests/integration/**",
    "test:integration": "vitest run -c vitest.integration.config.ts",
    "type-check": "tsc --noEmit",
    "verify": "tsx scripts/verify.ts"
  },
  "keywords": [
    "jules",
    "ai",
    "agent",
    "sdk",
    "developer-tools"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@google/jules": "^0.1.37",
    "@types/node": "^24.9.1",
    "msw": "^2.11.6",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
