{
  "name": "julets",
  "version": "0.0.1",
  "description": "The agent-ready SDK for Jules.",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:integration": "vitest run -c vitest.integration.config.ts",
    "type-check": "tsc --project tsconfig.test.json",
    "verify": "tsx scripts/verify.ts",
    "mcp:client": "tsx scripts/mcp-client.ts",
    "prepare": "husky"
  },
  "keywords": [
    "jules",
    "ai",
    "agent",
    "sdk",
    "developer-tools"
  ],
  "author": "",
  "license": "ISC",
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx,json,md,css}": "prettier --write"
  },
  "devDependencies": {
    "@google/jules": "^0.1.37",
    "@types/node": "^24.9.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "msw": "^2.11.6",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.2",
    "express": "^5.1.0"
  }
}
