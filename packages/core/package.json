{
  "name": "modjules",
  "version": "0.1.1",
  "type": "module",
  "description": "The agent-ready SDK for Jules.",
  "repository": "davideast/modjules",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./browser": {
      "types": "./dist/browser.d.ts",
      "import": "./dist/browser.mjs"
    },
    "./gas": {
      "types": "./dist/gas/index.d.ts",
      "import": "./dist/gas/index.mjs"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server.mjs"
    },
    "./server/auth/firebase": {
      "types": "./dist/server/auth/firebase.d.ts",
      "import": "./dist/server/auth/firebase.mjs"
    },
    "./server/auth/memory": {
      "types": "./dist/server/auth/memory.d.ts",
      "import": "./dist/server/auth/memory.mjs"
    },
    "./server/auth/rbac": {
      "types": "./dist/server/auth/rbac.d.ts",
      "import": "./dist/server/auth/rbac.mjs"
    },
    "./mcp": {
      "types": "./dist/mcp/index.d.ts",
      "import": "./dist/mcp.mjs"
    },
    "./mcp/cli": {
      "types": "./dist/mcp/cli.d.ts",
      "import": "./dist/mcp/cli.mjs"
    },
    "./types": {
      "types": "./dist/types.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "bin": {
    "modjules-mcp": "./dist/mcp/cli.mjs"
  },
  "scripts": {
    "build": "vite build",
    "test": "vitest --exclude tests/storage/browser.test.ts",
    "type-check": "tsc --project tsconfig.test.json"
  },
  "keywords": [
    "jules",
    "ai",
    "agent",
    "sdk",
    "developer-tools"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@inquirer/prompts": "^7.10.1",
    "@modelcontextprotocol/sdk": "^1.25.1",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "idb": "^8.0.3"
  },
  "peerDependencies": {
    "firebase-admin": ">=12.0.0"
  },
  "peerDependenciesMeta": {
    "firebase-admin": {
      "optional": true
    }
  }
}
